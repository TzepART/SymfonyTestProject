<div class="row">
    {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
    <div class="col-md-4 col-md-offset-4 create_register_form">
        <h2 class="register_title">Registration</h2>
        {{ form_widget(form) }}
        <div class="form-group">
            <input type="submit" class="btn login-btn btn-primary register-btn" value="Registration"/>
        </div>
    </div>
    {{ form_end(form) }}
</div>

<style>
    .register_title{
        text-align: center;
    }
    .register-btn{
        width: 100%;
    }
</style>
